<process>
  <model>    
  </model>
  <dml>
    <unit id="TcJomApartadosDto">
      <select id="row">
        select      
          *
        from      
          tc_jom_apartados
        where 
          {condicion} 
      </select>
      <select id="identically">
        select      
          *
        from      
          tc_jom_apartados
        where      
          id_apartado = {idApartado}
      </select>
    </unit>
